<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-reverse-engineering SYSTEM "http://hibernate.sourceforge.net/hibernate-reverse-engineering-3.2.2.GA-20081116.dtd">
<hibernate-reverse-engineering>
  <table-filter match-name="BIN$.*" match-schema="PUBLIC" exclude="true" />
  <table-filter match-name="SCHEMA_VERSION.*" match-schema="PUBLIC" exclude="true" />

  <!-- ======================================================================== -->

  <table schema="PUBLIC" catalog="PUBLIC" name="U_USER_BASE" class="com.mossle.user.domain.UserBase">
    <primary-key>
      <generator class="native"/>
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>

  <table schema="PUBLIC" catalog="PUBLIC" name="U_USER_EXTRA" class="com.mossle.user.domain.UserExtra">
    <primary-key>
      <generator class="native"/>
      <key-column name="ID" type="java.lang.Long"/>
    </primary-key>
  </table>

</hibernate-reverse-engineering>

